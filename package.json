{
  "name": "react-mui-starterkit",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "e2e": "playwright test && npx playwright show-report test-results/e2e-report",
    "e2e:ui": "playwright test --ui",
    "test": "vitest run --coverage",
    "dev": "vite",
    "build": "npm run lint && npm run test && tsc -b && vite build",
    "build:stg": "npm run lint && npm run test && tsc -b && vite build --mode stg",
    "build:prod": "npm run lint && npm run test && tsc -b && vite build --mode prod",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "preview": "vite preview",
    "api:gen": "npx openapi-generator-cli generate -i src/api/api.yml -g typescript-axios -o ./src/api/generated",
    "api": "npx @stoplight/prism-cli mock --dynamic src/api/api.yml --verboseLevel trace"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.2.5",
    "@hookform/resolvers": "^5.0.1",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@mui/x-data-grid": "^8.2.0",
    "@mui/x-date-pickers": "^8.2.0",
    "@mui/x-tree-view": "^8.2.0",
    "@tanstack/react-query": "^5.73.3",
    "axios": "^1.8.4",
    "date-fns": "^4.1.0",
    "lodash": "^4.17.21",
    "loglevel": "^1.9.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.55.0",
    "react-router": "^7.5.0",
    "react-router-dom": "^7.6.0",
    "uuid": "^11.1.0",
    "zod": "^3.25.12",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@openapitools/openapi-generator-cli": "^2.19.0",
    "@playwright/test": "^1.51.1",
    "@react-router/dev": "^7.5.0",
    "@remix-run/router": "^1.23.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.1.1",
    "eslint": "^9.21.0",
    "eslint-import-resolver-typescript": "^4.3.5",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "playwright": "^1.51.1",
    "ts-jest": "^29.3.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vite-plugin-css": "^1.0.4",
    "vite-plugin-static-copy": "^3.0.0",
    "vitest": "^3.1.1"
  }
}
